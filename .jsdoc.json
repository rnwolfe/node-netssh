{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["README.md", "./lib"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "template": "assets/template/docdash/",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  }
}
